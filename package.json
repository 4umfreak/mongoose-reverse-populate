{
  "name": "mongoose-reverse-populate-v2",
  "version": "1.2.0",
  "description": "This module allows you to 'populate' a mongoose model where the relationship ids are stored on another mongoose model that is related to this model",
  "main": "index.js",
  "scripts": {
    "deploy": "np",
    "test": "cross-env NODE_ENV=testing DEBUG='-mongoose-reverse*' nyc mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/4umfreak/mongoose-reverse-populate.git"
  },
  "keywords": [
    "mongo",
    "mongoose",
    "populate"
  ],
  "author": "Simon Taylor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/4umfreak/mongoose-reverse-populate/issues"
  },
  "homepage": "https://github.com/4umfreak/mongoose-reverse-populate",
  "devDependencies": {
    "async": "^2.6.1",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mongoose": "^5.4.5",
    "np": "^4.0.1",
    "nyc": "^13.1.0"
  },
  "dependencies": {
    "debug": "^4.1.1"
  },
  "engines": {
    "node": ">= 6"
  }
